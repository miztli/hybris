<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
                   id="com.hybris.cockpitng.visjs.networkchart">
    <name>Vis.js Network Chart</name>
    <description>Vis.js Network Chart</description>
    <defaultTitle>Vis.js Network Chart</defaultTitle>
    <author>SAP Hybris</author>
    <version>1.0</version>
    <view src="networkchart.zul" />
    <controller class="com.hybris.backoffice.widgets.networkchart.NetworkChartController"/>

    <sockets>
        <input id="initData" type="java.lang.Object"/>
        <input id="updateObject" type="java.lang.Object"/>
     </sockets>

    <keywords>
        <keyword>Charts</keyword>
    </keywords>
    <settings>
        <setting key="chartOptions" default-value="{}" type="java.lang.String"/>
        <setting key="networkPopulator" default-value="" type="java.lang.String"/>
        <setting key="viewEventHandler" default-value="" type="java.lang.String"/>
        <setting key="dataManipulationHandler" default-value="" type="java.lang.String"/>
    </settings>
</widget-definition>
