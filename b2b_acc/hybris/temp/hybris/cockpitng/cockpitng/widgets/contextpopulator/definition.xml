<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	id="com.hybris.cockpitng.contextpopulator"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd">

	<name>Context Populator</name>
	<description>Context populator widget can be used to populate context map based on any object.</description>
	<defaultTitle>Context populator</defaultTitle>
	<author>SAP hybris</author>
	<version>1.0</version>
	<view src="none" />
	<keywords>
		<keyword>Adapters</keyword>
	</keywords>

	<sockets>
		<input id="data" type="java.lang.Object" />
		<output id="context" type="java.util.Map" />
	</sockets>

	<controller
		class="com.hybris.backoffice.widgets.contextpopulator.ContextPopulatorController" />

	<settings>
		<setting key="contextPopulator" default-value="contextPopulator" type="java.lang.String"/>
	</settings>

</widget-definition>
