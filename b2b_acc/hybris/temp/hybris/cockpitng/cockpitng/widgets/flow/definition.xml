<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
	id="com.hybris.cockpitng.flowcontainer">

	<name>Flow Container</name>
	<description>Default flow container widget.</description>
	<view src="flowcontainer.zul"/>
	<keywords>
		<keyword>Wizard</keyword>
	</keywords>
	<sockets>
		<input type="java.lang.Object" id="start" />
		<input type="java.lang.Object" id="endInternal" visibility="internal" />
		<output type="java.lang.Object" id="startInternal" visibility="internal"/>
		<output type="java.lang.Object" id="end" />
	</sockets>
	<controller class="com.hybris.cockpitng.widgets.controller.DefaultFlowWidgetController" />
</widget-definition>
