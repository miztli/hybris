<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.zkoss.org/2005/zul"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:zk="http://www.zkoss.org/2005/zk"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.hybris.com/schema/cockpitng/zul/zul.xsd "
	height="100%">

	<hlayout id="mainCnt" sclass="yw-collection-preview-cnt">
		<div sclass="yw-collection-preview-center" vflex="1" style="width:auto">
			<button id="previous" sclass="z-paging-button z-paging-prev yw-preview-previous" visible="${widgetSettings.navigationOn}"/>
				<widgetslot slotID="editorArea" height="100%" width="100%" id="editorArea"/>
			<button id="next" sclass="z-paging-button z-paging-next yw-preview-next" visible="${widgetSettings.navigationOn}"/>
		</div>
		<div id="east" vflex="1" sclass="yw-collection-preview-east" visible="false">
			<widgetslot id="list" slotID="list" width="100%" height="100%"/>
		</div>
   </hlayout>
</widget>