<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.zkoss.org/2005/zul"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns:h="http://www.w3.org/1999/xhtml" 
		xmlns:zk="http://www.zkoss.org/2005/zk"
		xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.hybris.com/schema/cockpitng/zul/zul.xsd " height="100%">
		
	<div vflex="1" id="gridContainer" sclass="yw-simplelist-gridcontainer" if='${!"List".equals(widgetSettings.viewMode)}'>
	</div>
	
	<div vflex="1" width="100%" sclass="yw-simplelist" if='${"List".equals(widgetSettings.viewMode)}' >
		<listbox vflex="1" hflex="1" id="gridList" emptyMessage="${labels.emptylist}"/>
	</div>
</widget>
