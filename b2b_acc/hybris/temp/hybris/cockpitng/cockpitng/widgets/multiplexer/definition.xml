<widget-definition id="com.hybris.cockpitng.widgets.common.multiplexer"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd">
	<name>Multiplexer</name>
	<description>
		Multiplexer is used to notify n of m (n &lt;= m) of its virtual output sockets.
		The notification decision is based on SpEL expression evaluation.
		For a virtual output socket "virtSocketX" the condition must be stored in a string widget setting named "virtSocketX_expression".
	</description>
	<defaultTitle>Multiplexer</defaultTitle>
	<author>hybris</author>
	<version>0.1</version>
	<sockets>
		<input id="element" type="java.lang.Object" />
	</sockets>
	<view src="none" />
	<controller
		class="com.hybris.cockpitng.widgets.common.multiplexer.MultiplexerController" />
	<keywords>
		<keyword>Adapters</keyword>
	</keywords>

</widget-definition>
